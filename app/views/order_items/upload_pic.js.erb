<% if @orderitem.image_uid.nil? %>
alert('The photo could not be uploaded: <%= j @orderitem.errors.full_messages.join(', ').html_safe %>');
<% else %>
$(".uuid").each(function( i ) {
if ( $(this).val()== "<%= @orderitem.itemUUID %>" ) {
	alert($(this).closest("tr").find(".img-td").html());
    $(this).closest("tr").find(".img-td").html('<img src="<%= @orderitem.image.thumb('180x180#').url %>" class="img-thumbnail" alt="Smiley face" height="30" width="30">');
    return;
}
});
<% end %>
